let destinations = ['Prague', 'London', 'Sydney', 'Belfast', 'Rome',
  'Aruba', 'Paris', 'Bora Bora', 'Barcelona', 'Rio de Janeiro',
  'Marrakesh', 'New York City'];

  function contains(string){
    destinations.forEach( function (place){
      console.log(string);
      console.log(place);
      if(place === string){
        console.log('yey');
        return true;
      }
    })
    return false;
  }
  


